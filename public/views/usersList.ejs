<%- include('templates/head'); %>

  <div class="app-container">
    <h2>All Users List</h2>
    <% if (users) { %> <% users.forEach(user => { %>
      <div class="user-container">
        <p><span>Username</span> : <%= user.username %></p>
        <p><span>ID</span> : <%= user._id %></p>
      </div>
    <% }) %> <% } %>
      <p style="text-align: center; margin-bottom: 1em !important">
        <span>Users</span> : <%= users.length %>
      </p>
      <a href="/users" class="btn">GO TO USERS</a>
      <div style="width: 100%; height: 2em;"></div>
  </div>

</body>
</html>